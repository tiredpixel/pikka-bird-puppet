# Defaults for pikka-bird-server, sourced by the init script.
# This file is dancing on a Puppet string; it's probably not a good idea to
# change it manually.
# 
# The config variables are based on `.env.example` in the repository.


DATABASE_URI=<%= @database_uri %>
IP=<%= @bind_ip %>
LOG_LEVEL=<%= @log_level %>
PORT=<%= @port %>
WORKERS=<%= @workers %>
